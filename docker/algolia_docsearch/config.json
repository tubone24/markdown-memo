{
  "index_name": "vuepress",
  "start_urls": [
    "https://note.tubone-project24.xyz/"
  ],
  "stop_urls": [],
    "selectors": {
        "lvl0": {
            "selector": "p.sidebar-heading.open",
            "global": true,
            "default_value": "Documentation"
        },
        "lvl1": "h1",
        "lvl2": "h2",
        "lvl3": "h3",
        "lvl4": "h4",
        "lvl5": "h5",
        "text": "p, li",
        "lang": {
            "selector": "/html/@lang",
            "type": "xpath",
            "global": true
        }
    },
  "strip_chars": " .,;:#",
  "custom_settings": {
    "attributesForFaceting": [
      "lang"
    ]
  },
  "scrap_start_urls": false
}
